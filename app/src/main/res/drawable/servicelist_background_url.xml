<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:startColor="@color/listitem_background"
        android:endColor="@color/listitem_background"
        android:centerColor="@color/listitem_background"
        android:angle="90"
        android:type="linear"/>
    <corners android:topLeftRadius="10dp"
        android:topRightRadius="10dp"
        android:bottomLeftRadius="10dp"
        android:bottomRightRadius="10dp"/>
    <padding
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp"/>
    <stroke android:width="2dp"
        android:color="@color/listitem_border"/>
</shape>
